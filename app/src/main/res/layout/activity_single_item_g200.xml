        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNewPaint" />

    <TextView
        android:id="@+id/txtSparkPlug"
        android:layout_width="wrap_content"
